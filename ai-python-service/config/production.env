# Production Environment Configuration
AI_SERVICE_DEBUG=false
EUREKA_SERVER_URL=http://eureka-server:8761/eureka
REDIS_HOST=redis
REDIS_PORT=6379
MODEL_CACHE_SIZE=200
MAX_CONCURRENT_REQUESTS=20
MODEL_TIMEOUT_SECONDS=15
MEMORY_LIMIT_MB=4096
PROMETHEUS_PORT=9090
LOG_LEVEL=WARNING

# Model Configuration
TASK_PARSER_MODEL=microsoft/DialoGPT-medium
TASK_PARSER_VERSION=v1
PRIORITIZER_MODEL=microsoft/DialoGPT-medium
PRIORITIZER_VERSION=v1
INSIGHTS_MODEL=microsoft/DialoGPT-medium
